    "vuepress-plugin-named-chunks": "1.1.4",
    "vuepress-plugin-smooth-scroll": "0.0.10"
  },
  "dependencies": {
    "vuepress-plugin-img-lazy": "1.0.4",
    "vuepress-plugin-reading-progress": "1.0.10",
